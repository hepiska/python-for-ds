    "def add(*args):\n",
    "    res = 0\n",
    "    for arg in args:\n",
    "        res += arg\n",
    "    return res\n",
    "\n",
    "def multiply(*args):\n",
    "    res = 1\n",
    "    for arg in args:\n",
    "        res = res * arg\n",
    "    return res\n",
    "\n",
    "def devide(*args):\n",
    "    [res, *sisa] = args\n",
    "    for arg in sisa:\n",
    "        res = res / arg\n",
    "    return res"
